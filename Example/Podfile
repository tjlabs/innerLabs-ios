use_frameworks!

platform :ios, '13.2'
# source 'https://github.com/tjlabs/jupiter-sdk-ios.git'

target 'JupiterSDK_Example' do
  use_frameworks!
  pod 'JupiterSDK', :path => '../'
  pod 'Alamofire'
  pod 'GoogleMaps', '4.0.0'
  pod 'GooglePlaces', '4.0.0'
  pod 'Charts' 
  pod 'SnapKit'
  pod 'ExpyTableView'
  pod 'Kingfisher'
  pod 'DropDown'
  pod 'Floaty', '~> 4.2.0'
  pod 'FirebaseMLCommon'
  pod 'Firebase/MLModelInterpreter', '6.25.0'
  pod 'TensorFlowLiteSwift'

  post_install do |installer|
    installer.pods_project.targets.each do |target|
      target.build_configurations.each do |config|
        config.build_settings['ONLY_ACTIVE_ARCH'] = 'YES'
        config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '13.2'
      end
    end
  end

  target 'JupiterSDK_Tests' do
    inherit! :search_paths

  end
end
